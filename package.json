{
  "name": "gtm-to-git",
  "version": "1.0.0",
  "description": "Create a filesystem from GTM, so you can save to a git repository. Eventually build and push your changes back to GTM.",
  "main": "index.js",
  "bin": {
    "gtm2git": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint ./src/**/*.ts",
    "lint:fix": "tslint --fix src/**/*.ts",
    "start": "ts-node ./src/index.ts",
    "start:dist": "node ./dist/index.js",
    "test": "mocha -r ts-node/register src/**/*spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctsstc/gtm-to-git.git"
  },
  "keywords": [
    "gtm",
    "git",
    "filesystem",
    "files"
  ],
  "author": "CTS_Ã†",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ctsstc/gtm-to-git/issues"
  },
  "homepage": "https://github.com/ctsstc/gtm-to-git#readme",
  "dependencies": {
    "commander": "^2.19.0",
    "dotenv": "^6.2.0",
    "googleapis": "^37.2.0",
    "readline-promise": "^1.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^6.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.1",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  }
}
